<div class="limiter">

  <p id="notice"><%= notice %></p>

  <span class="login100-form-title p-b-70 text-left">
      Trips
  </span>

  <div class="container-login100">

    <div class="wrap-login100 p-t-85 p-b-20">

      <span class="login100-form-avatar">
        <%= image_tag('bike_tipo_vc_logo.png') %>
      </span>

      <table>
        <thead>
        <tr>
          <th colspan="3"></th>
        </tr>
        </thead>

        <tbody>
        <% @trips.each do |trip| %>
          <tr>
            <td><%= link_to 'Show', trip %></td>
          </tr>
        <% end %>
        </tbody>
      </table>

    </div>

  </div>

</div>

